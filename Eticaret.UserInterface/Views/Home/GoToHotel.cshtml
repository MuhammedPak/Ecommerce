    <script>
        $('.carousel').carousel({
            interval: 1000
        });

        function GetRoomId(id) {
            $("#GetRoomId").val(id);
        }

        $(function () {
            $("#startdate").datepicker({ dateFormat: "dd-mm-yy" }).val()
            $("#enddate").datepicker({ dateFormat: "dd-mm-yy" }).val()
        });

   
        $("input").keyup(function () {
            if ($("input").val() != "") {

                $.ajax({
                    type: "Get",
                    url: "/Home/Search",
                    data: { "key": $("input").val() }

                }).done(function (data) {

                    $("#list").html(data);
                    //$("#list").append('<li><a href="/Home/PreviewRoom" ><span class="title">"' + data[0].HotelName + '"</span ></a ></li >');
                    $.each(data, function (index) {
                        $("#list").append(data[index])
                        $("#list").append('<li class="form-control"><a href="/Home/GoToHotel/' + data[index].hotelId + '" ><span class="title" style="font-size:18px;">' + data[index].HotelName + '</span ></a ></li >');
                    });



                });

            } else {
                $("#list").html(" ");

            }
        });


